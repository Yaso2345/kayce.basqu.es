{
  "name": "kaycebasques",
  "version": "1.0.0",
  "description": "personal website of the artist formerly known as kayce",
  "private": "true",
  "watch": {
    "build": {
      "patterns": [
        "**/*"
      ],
      "extensions": "html,css,md,png,gif,mp4,ttf,json,sh",
      "ignore": [
        "build/**/*",
        "node_modules/**/*"
      ]
    }
  },
  "scripts": {
    "start": "node build/server.js;",
    "build": "build/main.sh;",
    "install": "npm run build;",
    "debug": "export DEBUG=true; npm run build;",
    "test": "npm run build;",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaycebasques/kaycebasques.git"
  },
  "author": "Kayce Basques",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaycebasques/kaycebasques/issues"
  },
  "homepage": "https://kayce.basqu.es",
  "devDependencies": {
    "clean-css": "^4.1.9",
    "express": "^4.16.3",
    "handlebars": "^4.0.11",
    "html-minifier": "^3.5.10",
    "npm-watch": "^0.5.0",
    "showdown": "^1.8.6",
    "uglify-js": "^3.3.12"
  },
  "dependencies": {}
}
